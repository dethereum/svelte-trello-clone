    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "open": "^7.0.4",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "style-loader": "^1.2.1",
