{
  "name": "svelte-tutorial",
  "version": "1.0.0",
  "description": "a repo to follow along with the guide on svelte.dev",
  "main": "webpack.config.js",
  "scripts": {
    "start": "node tools/start.js",
    "start:prod": "pnpm run build && node tools/startProd.js",
    "build": "webpack --config=webpack.config.prod.js",
    "preinstall": "npx only-allow pnpm",
    "test": "jest src"
  },
  "engines": {
    "node": ">=14.5.0",
    "pnpm": ">=5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dethereum/svelte-tutorial.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dethereum/svelte-tutorial/issues"
  },
  "homepage": "https://github.com/dethereum/svelte-tutorial#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm run test"
    }
  },
  "lint-staged": {
    "*.svelte": [
      "eslint",
      "prettier --plugin-search-dir=."
    ],
    "*.js": [
      "eslint",
      "prettier"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "svelte": "3.24.0",
    "svelte-match-media": "^1.1.0",
    "sveltestrap": "^3.5.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@sucrase/jest-plugin": "^2.0.0",
    "@testing-library/svelte": "^3.0.0",
    "@types/jest": "^26.0.5",
    "bundle-stats-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "express": "^4.17.1",
    "favicons-webpack-plugin": "^3.0.1",
    "file-loader": "^6.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "open": "^7.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "purgecss-from-svelte": "^2.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "spdy": "^4.0.2",
    "svelte-jester": "^1.0.6",
    "svelte-loader": "^2.13.6",
    "svelte-loader-hot": "^0.3.0",
    "svelte-preprocess": "^4.0.8",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-fix-style-only-entries": "^0.5.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.0.9"
  }
}
